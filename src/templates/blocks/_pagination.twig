<nav aria-label="Page navigation example">
	<ul class="pagination">

		<li class="page-item">
			<a class="page-link" href="#">Previous</a>
		</li>

		{% for i in 1..5 %}
			<li class="page-item{{ (loop.first) ? ' active' : '' }}">
				<a class="page-link" href="#">{{ i }}</a>
			</li>
		{% endfor %}

		<li class="page-item">
			<a class="page-link" href="#">Next</a>
		</li>

	</ul>
</nav>
